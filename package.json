{
  "name": "@adviser/ts-xxhash",
  "version": "1.0.0",
  "description": "xxHash in Typescript",
  "main": "./cjs/index.js",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "require": "./cjs/index.cjs",
      "import": "./esm/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "pnpm run build:tsgo; pnpm run build:js; pnpm run build:deno",
    "build:tsc": "rm -rf ./dist/ts ; pnpm run /^build:tsc:/",
    "build:tsgo": "rm -rf ./dist/ts ; pnpm run /^build:tsgo:/",
    "build:tsgo:global": "tsgo --noEmit",
    "build:tsgo:esm": "tsgo -p ./tsconfig.esm.json",
    "build:tsgo:cjs": "tsgo -p ./tsconfig.cjs.json",
    "build:js": "rm -rf dist/pkg ; mkdir -p dist/pkg; cp -pr ./dist/ts/* dist/pkg",
    "build:deno": "deno publish --dry-run --unstable-sloppy-imports --allow-dirty --set-version 0.0.0-dev",
    "test": "pnpm run test:js; pnpm run test:deno",
    "test:deno": "deno run --quiet --allow-net --allow-write --allow-run  --allow-sys --allow-ffi  --allow-read --allow-env  ./node_modules/vitest/vitest.mjs --run --project node",
    "test:js": "vitest --run",
    "bench": "vitest bench --project node --project browser --run test/bench.bench.ts",
    "lint": "eslint .",
    "pubdir": "cement preparePubdir --baseDir . --srcDir lib",
    "presmoke": "pnpm run pubdir ; cd pubdir ; pnpm pack",
    "deno:publish": "pnpm run pubdir; cd pubdir ; pnpm exec deno publish --allow-dirty --unstable-sloppy-imports",
    "smoke": "bash ./smoke/it.sh",
    "format": "prettier ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mabels/ts-xxhash"
  },
  "keywords": [
    "xxhash",
    "xxh"
  ],
  "author": "Pierre Curto and Meno Abels",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mabels/ts-xxhash/issues"
  },
  "homepage": "https://github.com/mabels/ts-xxhash",
  "dependencies": {},
  "devDependencies": {
    "@adviser/cement": "^0.4.33",
    "@cloudflare/vitest-pool-workers": "^0.8.71",
    "@eslint/js": "^9.35.0",
    "@typescript/native-preview": "7.0.0-dev.20250909.1",
    "@vitest/browser": "^3.2.4",
    "cmd-ts": "^0.14.1",
    "deno": "^2.4.4",
    "eslint": "^9.35.0",
    "jscodeshift": "^17.3.0",
    "playwright": "^1.55.0",
    "playwright-chromium": "^1.55.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.43.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.35.0",
    "zx": "^8.8.1"
  }
}
